{
  "name": "wetblob",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev:infra": "docker compose up -d",
    "dev:down": "docker compose down",
    "db:migrate": "pnpm -C packages/lake-db migrate",
    "test": "pnpm -r test",
    "dev:gateway": "pnpm -C packages/lake-gateway dev",
    "build:gateway": "pnpm -C packages/lake-gateway build",
    "lake:list": "tsx packages/lake-cli/scripts/list.ts",
    "lake:collect": "tsx packages/lake-cli/scripts/run-collector.ts"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
